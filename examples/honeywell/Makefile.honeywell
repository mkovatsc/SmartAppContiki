all: honeywell

CONTIKI = ../..

# variable for root Makefile.include
UIP_CONF_IPV6=1

# radio configuration
CFLAGS += -DRF_CHANNEL=21
CFLAGS += -DIEEE802154_CONF_PANID=0xBEEF

# must be CFLAGS not variable
CFLAGS += -DUIP_CONF_IPV6_RPL=1
CFLAGS += -DUIP_CONF_TCP=1 # enabled by default, just as a reminder that it is needed for telnet 


# set the baud rate
CFLAGS += -DBAUD_RATE=USART_BAUD_9600

include $(CONTIKI)/Makefile.include
